  "scripts": {
    "clean": "./scripts/build/_rescript-wrapper.sh -clean-world",
    "build:rescript": "./scripts/build/_rescript-wrapper.sh -make-world",
    "build:app": "browserifyinc --cachefile lib/browserify-app-cache.json lib/js/client/src/app/Main.bs.js client/src/appsupport.js fsharp-backend/src/Analysis/static/BlazorWorker.js --standalone app -o backend/static/app.js",
