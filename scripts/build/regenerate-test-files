#!/usr/bin/env bash
. ./scripts/devcontainer/_assert-in-container "$0" "$@"

set -euo pipefail


jsonfile=backend/serialization/oplist-format-latest.json
binfile=backend/serialization/oplist-format-latest.bin

echo "Before: "
sha1sum "$jsonfile"
sha1sum "$binfile"

echo -e "\n\nRegenerating: "
cd backend
Build/out/Tests/Debug/net7.0/linux-x64/Tests --regenerate-test-files
cd ..

echo -e "\n\nAfter: "
sha1sum "$jsonfile"
sha1sum "$binfile"
