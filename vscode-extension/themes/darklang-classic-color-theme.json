  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#b8b8b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#c7abcd"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a1b56c"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#dc9656"
      }
    },
    {
      "name": "Pipe Operators",
      "scope": [
        "keyword.operator.pipe"
      ],
      "settings": {
        "foreground": "#7cafc2"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.numeric",
        "constant.character"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Booleans",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#a1b56c"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#7cafc2"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#dc9656"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ab4642"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#7cafc2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e8e8e8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#e8e8e8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7cafc2"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#c7abcd"
      }
    }
  ]
}
