{
  "name": "client",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "client/src",
      "subdirs": true
    },
    {
      "dir": "client/test",
      "type": "dev"
    },
    {
      "dir": "client/workers",
      "subdirs": false
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "ppx-flags": [
    ["@greenlabs/res-tailwindcss/ppx", "--path backend/static/app.css"]
  ],

  "suffix": ".bs.js",
  "bs-dependencies": [
    "bucklescript-tea",
    "@glennsl/bs-json",
    "rescript-webapi",
    "tablecloth-rescript",
    "bs-fetch",
    "bs-uuid",
    "rescript-js",
    "rescript-nodejs"
  ],
  "bs-dev-dependencies": [],
  "bsc-flags": [
    "-bs-g",
    "-bs-super-errors",
    "-warn-error",
    "@A",
    "-w",
    "-4-30-40-41-42-102"
  ],
  "warnings": {
    "error": "+101"
  }
}
