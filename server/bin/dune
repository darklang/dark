(executable
  (name darkjs)
  (public_name darkjs)
  (modules darkjs)
  (flags (-no-check-prims))
  (js_of_ocaml
    (flags (--opt=1
              --setenv FORCE_DROP_INLINE_TEST=true
              +nat.js
              +dynlink.js
              +toplevel.js)))
  (libraries js_of_ocaml libfrontend)
  (preprocess (pps js_of_ocaml-ppx))
  (package dark)
)

(alias
  (name darkjs)
  (deps darkjs.bc.js (universe))
  (action (system "cp darkjs.bc.js ../../../static/")))


