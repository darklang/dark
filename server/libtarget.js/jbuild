(jbuild_version 1)

(library
 ((name libtarget_js)
  (wrapped false)
  (libraries (core_kernel js_of_ocaml))
  (flags (-warn-error (+A) :standard -no-keep-locs))
  (js_of_ocaml
    ((flags (+nat.js
             +core_kernel/runtime.js
             --opt 1))
    (javascript_files (libtarget.js))))))


(rule (copy# ${lib:libtarget:libtarget.mli} libtarget.mli))
