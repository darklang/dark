    ((Builtin.Test.getCanvasID ())
     |> Builtin.DarkInternal.Canvas.Domain.get
     |> PACKAGE.Darklang.Stdlib.List.head
     |> Builtin.unwrap
     |> Builtin.DarkInternal.Canvas.Domain.toCanvasID) = PACKAGE
      .Darklang
      .Stdlib
      .Result
      .Result
      .Ok(Builtin.Test.getCanvasID ())

