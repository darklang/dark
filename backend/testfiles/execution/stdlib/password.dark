// End-user password hashing and checking works
Builtin.Password.check_v0 (Builtin.Password.hash_v0 "password") "password" = true

Builtin.Password.check_v0
  (Builtin.Password.hash_v0 "👱👱🏻👱🏼👱🏽👱🏾👱")
  "👱👱🏻👱🏼👱🏽👱🏾👱" = true

Builtin.Password.check_v0 (Builtin.Password.hash_v0 "") "" = true

// Password hashes do not equal each other
Builtin.Password.hash_v0 "my password" == Builtin.Password.hash_v0 "my password" = false
Builtin.Password.hash_v0 "" == Builtin.Password.hash_v0 "" = false

Builtin.Password.hash_v0 "my password"
== Builtin.Password.hash_v0 "not my password" = false