    "val1"

  Builtin.Dict.get (Dict { key1 = "val1" }) "" = PACKAGE.Darklang.Stdlib.Option.Option.None


module IsEmpty =
  Builtin.Dict.isEmpty_v0 (Dict { a = 1 }) = false
  Builtin.Dict.isEmpty_v0 (Dict { }) = true


module Keys =
  Builtin.Dict.keys_v0 (Dict { key1 = "val1" }) = [ "key1" ]


module Map =
  Builtin.Dict.map_v0 (Dict { key1 = "val1"; key2 = "val2" }) (fun (k, x) -> k ++ x) = (Dict
    { key2 = "key2val2"; key1 = "key1val1" })

  Builtin.Dict.map_v0 (Dict { key1 = 5; key2 = 3; key3 = 3 }) (fun (k, x) ->
    PACKAGE.Darklang.Stdlib.Bool.and_v0
      (Builtin.Int.greaterThanOrEqualTo_v0 x 1)
      (Builtin.Int.lessThanOrEqualTo_v0 x 4)) = (Dict
    { key3 = true
      key2 = true
      key1 = false })

  Builtin.Dict.map_v0 (Dict { a = 1; b = 2 }) (fun (k, x) -> x + 1) = (Dict
    { b = 3; a = 2 })

  Builtin.Dict.map_v0 (Dict { }) (fun (key, value) -> 0) = (Dict { })


module Member =
