module Empty =
  Builtin.Dict.empty_v0 = Dict { }


module FilterMap =
  Builtin.Dict.filterMap_v0 (Dict { }) (fun (key, value) -> 0) = (Dict { })

  Builtin.Dict.filterMap_v0 (Dict { a = "x"; b = "y"; c = "z" }) (fun (key, value) ->
    if value == "y" then
      PACKAGE.Darklang.Stdlib.Option.Option.None
    else
      (PACKAGE.Darklang.Stdlib.Option.Option.Some(key ++ value))) = (Dict
    { c = "cz"; a = "ax" })

  (Builtin.Dict.filterMap_v0
    (Dict { a = "x"; b = "y"; c = "z" })
    (fun (key, value) ->
      if value == "y" then
        false
      else
        (PACKAGE.Darklang.Stdlib.Option.Option.Some(key ++ value)))) = (Builtin
    .Test
    .derrorMessage (
      "Expected `fn` to return a "
      ++ "PACKAGE.Darklang.Stdlib.Option.Option<'b>,"
      ++ " but it returned `false`"
    ))


module Filter =
  Builtin.Dict.filter (Dict { key1 = "val1"; key2 = "val2" }) (fun (k, v) ->
    k == "key1") = (Dict { key1 = "val1" })

  Builtin.Dict.filter (Dict { key1 = 1; key2 = 3 }) (fun (k, v) -> v < 2) = (Dict
    { key1 = 1 })

  Builtin.Dict.filter (Dict { }) (fun (k, v) -> 0) = (Dict { })

  Builtin.Dict.filter (Dict { a = 1; b = 2; c = 3 }) (fun (k, v) -> 2) = Builtin.Test.derrorMessage
    "Expected `fn` to return a Bool, but it returned `2`"


