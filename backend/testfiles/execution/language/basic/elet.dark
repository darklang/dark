  (let ((a, ((b, cd), e)), f) = ((1L, ((2L, (3L, 4L)), 5L)), 6L) in cd) = (3L, 4L)


module Nesting =
  (let x =
    let y = 1L
    let z = 2L
    y + z

   x) = 3L


