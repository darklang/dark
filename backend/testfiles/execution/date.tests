[fn.d datestr:string]
(Date.parse_v2 datestr) |> Test.unwrap

[fn.p datestr:string]
(Date.parse_v2 datestr) |> Result.map_v1 (fun x -> Date.toString_v0 x)

[tests.date parsing]
(Date.parse_v2 "2019-07-28T22:42:36Z") |> Result.map_v1 (fun x -> Date.toString_v0 x) = Ok "2019-07-28T22:42:36Z"
