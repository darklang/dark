  | () -> "()"
  | Ok x -> "ok: " ++ x
  | Nothing -> "constructor nothing"
  | 1.1 -> "float"
  | (1, 2) -> "1 and 2"
  | [i] -> "list:" ++ i
  | name -> name ++ "var") = "float"

  // bool
