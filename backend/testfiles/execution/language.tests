  | 5.6 -> "float"
  | "myStr" -> "string"
  | () -> "()"
  | Ok x -> "ok: " ++ x
  | Nothing -> "constructor nothing"
  | false -> "bool"
  | (1, 2) -> "1 and 2"
  | [i] -> "list:" ++ i
