[expected-request]
GET PATH?i=5&n=-1 HTTP/1.1
Host: HOST
Content-Length: 0


[response]
HTTP/1.1 200 OK
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Content-type: text/plain; charset=utf-8
Content-Length: LENGTH

Hello back

[test]
(let response = (HttpClient.request "get" "http://URL?i=5&n=-1" [] Bytes.empty) |> Test.unwrap
 let respHeaders = response.headers |> List.filter_v2 (fun h -> Tuple2.first h != "date")
 Dict.set_v0 response "headers" respHeaders) =
   { statusCode = 200
     headers =
      [
        ("server", "kestrel")
        ("content-length", "LENGTH")
        ("content-type", "text/plain; charset=utf-8")
      ]
     body = "Hello back" |> String.toBytes }
