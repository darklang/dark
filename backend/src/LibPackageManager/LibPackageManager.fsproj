<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <!-- Publishing configuration -->
    <IsPublishable>false</IsPublishable>
    <IsTrimmable>true</IsTrimmable>
  </PropertyGroup>
  <ItemGroup>
    <None Include="paket.references" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../Prelude/Prelude.fsproj" />
    <ProjectReference Include="../LibExecution/LibExecution.fsproj" />
    <ProjectReference Include="../LibSerialization/LibSerialization.fsproj" />
    <ProjectReference Include="../LibConfig/LibConfig.fsproj" />
    <ProjectReference Include="../LibDB/LibDB.fsproj" />
  </ItemGroup>
  <ItemGroup>
    <!-- Helpers /-->
    <Compile Include="Caching.fs" />


    <!-- RT /-->
    <Compile Include="RT/SQL.fs" />


    <!-- PT /-->
    <Compile Include="PT/Compose.fs" />
    <Compile Include="PT/InMemory.fs" />
    <Compile Include="PT/Empty.fs" />

    <Compile Include="PT/SQL/Instances.fs" />
    <Compile Include="PT/SQL/Branches.fs" />

    <Compile Include="PT/SQL/Types.fs" />
    <Compile Include="PT/SQL/Values.fs" />
    <Compile Include="PT/SQL/Fns.fs" />
    <Compile Include="PT/SQL/Search.fs" />

    <Compile Include="PT/SQL/OpPlayback.fs" />

    <Compile Include="PT/SQL/PM.fs" />
    <!-- (maybe these should be part of the PM type, idk) -->
    <Compile Include="PT/SQL/Stats.fs" />
    <Compile Include="PT/SQL/Purge.fs" />
    <Compile Include="PT/SQL/Scripts.fs" />
    <Compile Include="PT/SQL/Sync.fs" />


    <!-- Round it all up in a nice package. -->
    <Compile Include="PackageManager.fs" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>