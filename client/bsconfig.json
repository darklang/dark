{
  "name": "client",
  "version": "0.1.0",
  "sources": [
    {
      "dir" : "src",
      "subdirs" : true 
    },
    {
      "dir": "__tests__",
      "type": "dev"
    },
    {
      "dir": "workers",
      "subdirs": false
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "ppx-flags": [
    "./run-ppxes"
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bucklescript-tea",
    "@glennsl/bs-json",
    "bs-webapi",
    "tablecloth-bucklescript",
    "bs-fetch",
    "bs-uuid"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "bsc-flags": ["-bs-g", "-bs-super-errors", "-warn-error", "@A", "-w", "-4-9-30-40-41-42-102"],
  "warnings": {
    "error": "+101",
  },
  "refmt": 3
}
