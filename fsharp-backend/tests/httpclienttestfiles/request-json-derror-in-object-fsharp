[expected-request]
POST PATH HTTP/1.1
Accept: */*
Accept-Encoding: deflate, gzip, br
Content-Type: application/json; charset=utf-8
Host: HOST

{
  "x": {
    "Error": "x"
  }
}
[response]
HTTP/1.1 200 OK
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Content-type: application/json; charset=utf-8
Content-Length: LENGTH

{ "x": { "Error": "x" } }

[test]
// FSHARPONLY
(let value = { x = Test.typeError_v0 "x" } in
 let response = HttpClient.post_v5_ster "http://URL" value {} HttpClient.jsonContentType_v0 in
 let respHeaders = response.headers |> Dict.remove_v0 "Date" in
 Dict.set_v0 response "headers" respHeaders) =
   { body = { x = { ``Error`` = "x" } }
     code = 200
     error = ""
     headers =
      {
        ``Content-Length`` = "LENGTH"
        ``Content-Type`` = "application/json; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
     raw = "{ \"x\": { \"Error\": \"x\" } }"
   }