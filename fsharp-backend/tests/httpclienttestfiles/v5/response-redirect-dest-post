[expected-request]
POST PATH HTTP/1.1
Accept: */*
Accept-Encoding: deflate, gzip, br
Content-Type: text/plain; charset=utf-8
Host: HOST

[response]
HTTP/1.1 200 OK
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Content-type: text/plain; charset=utf-8
Arbitrary-header: Test value
Content-Length: LENGTH

Redirect destination reached

[test]
// This is here to be a destination for valid redirect tests
(let response = HttpClient.post_v5_ster "http://URL" "" {} {} in
 let respHeaders = response.headers |> Dict.remove_v0 "Date" in
 Dict.set_v0 response "headers" respHeaders) =
   { body = "Redirect destination reached"
     code = 200
     error = ""
     headers =
      {
        ``Arbitrary-header`` = "Test value"
        ``Content-Length`` = "LENGTH"
        ``Content-Type`` = "text/plain; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
     raw = "Redirect destination reached"}
