[test]
(let reqBody = { x = 6; y = 3.4; z = ["string"]} in
 let response = HttpClient.post "http://URL" reqBody {} HttpClient.jsonContentType_v0 in
 let respHeaders = response.headers |> Dict.remove_v0 "Date" in
 Dict.set_v0 response "headers" respHeaders) =
   { body = "\"Hello back\""
     headers =
      {
        ``Content-Length`` = "LENGTH"
        ``Content-Type`` = "text/plain; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
     raw = "\"Hello back\""}

