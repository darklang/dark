[expected-request]
GET PATH?l=1,2,a%20string,%5B%206%20%5D HTTP/1.1
Accept: */*
Accept-Encoding: deflate, gzip, br
Content-Type: application/json; charset=utf-8
Host: HOST


[response]
HTTP/1.1 200 OK
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Content-type: text/plain; charset=utf-8
Content-Length: LENGTH

"Hello back"

[test]
(let response = HttpClient.get "http://URL" {} { l = [1;2;"a string"; [6]] } HttpClient.jsonContentType_v0 in
 let respHeaders = response.headers |> Dict.remove_v0 "Date" in
 Dict.set_v0 response "headers" respHeaders) =
   { body = "\"Hello back\""
     headers =
      {
        ``Content-Length`` = "LENGTH"
        ``Content-Type`` = "text/plain; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
     raw = "\"Hello back\""}
