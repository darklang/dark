[fn.canonicalizeOCamlGoodResponseX result:result]
result |> Result.map_v1
  (fun response ->
    let headers =
      (Test.httpResponseHeaders response)
      |> List.map (fun h ->
        match h |> List.head_v0 with
        | "expires" -> ["expires"; "xxx, xx xxx xxxx xx:xx:xx xxx"]
        | "date" -> ["date"; "xxx, xx xxx xxxx xx:xx:xx xxx"]
        | "age" -> ["age"; "xxxx,cache-control: public, max-age=3600"]
        | "x-guploader-uploadid" -> ["x-guploader-uploadid"; "xxx"]
        | _ -> h
      )
    { code = Test.httpResponseStatusCode response; body = Test.httpResponseBody response; headers = headers})

[fn.canonicalizeOCamlGoodResponse result:result]
result
|> Result.map_v1
  (fun s ->
    s
    |> toString_v0
    |> Test.regexReplace "expires: \\w{3}, \\d\\d \\w{3} \\d\\d\\d\\d \\d\\d:\\d\\d:\\d\\d \\w{3}" "expires: xxx, xx xxx xxxx xx:xx:xx xxx"
    |> Test.regexReplace "date: \\w{3}, \\d\\d \\w{3} \\d\\d\\d\\d \\d\\d:\\d\\d:\\d\\d \\w{3}" "date: xxx, xx xxx xxxx xx:xx:xx xxx"
    |> Test.regexReplace "x-guploader-uploadid: [-_a-zA-Z0-9]+" "x-guploader-uploadid: xxx"
    |> Test.regexReplace "age: \d+" "age: xxxx")

[fn.canonicalizeOCamlBadResponse result:result]
result
|> Result.map_v1
  (fun s ->
    s
    |> toString_v0
    |> Test.regexReplace "expires: \\w{3}, \\d\\d \\w{3} \\d\\d\\d\\d \\d\\d:\\d\\d:\\d\\d \\w{3}" "expires: xxx, xx xxx xxxx xx:xx:xx xxx"
    |> Test.regexReplace "date: \\w{3}, \\d\\d \\w{3} \\d\\d\\d\\d \\d\\d:\\d\\d:\\d\\d \\w{3}" "date: xxx, xx xxx xxxx xx:xx:xx xxx"
    |> Test.regexReplace "x-guploader-uploadid: [-_a-zA-Z0-9]+" "x-guploader-uploadid: xxx"
    // in good responses, last-modified is a concrete value
    |> Test.regexReplace "last-modified: \\w{3}, \\d\\d \\w{3} \\d\\d\\d\\d \\d\\d:\\d\\d:\\d\\d \\w{3}" "last-modified: xxx, xx xxx xxxx xx:xx:xx xxx"
    |> Test.regexReplace "age: \d+" "age: xxxx")

